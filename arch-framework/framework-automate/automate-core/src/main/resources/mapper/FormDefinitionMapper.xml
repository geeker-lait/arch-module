<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.arch.framework.automate.from.mapper.FormDefinitionMapper">

    <resultMap type="org.arch.framework.automate.from.entity.FormDefinition" id="FormDefinitionMap">
        <id property="id" column="id" jdbcType="INTEGER"/>
        <result property="category" column="category" jdbcType="VARCHAR"/>
        <result property="formName" column="form_name" jdbcType="VARCHAR"/>
        <result property="formCode" column="form_code" jdbcType="VARCHAR"/>
        <result property="tableName" column="table_name" jdbcType="VARCHAR"/>
        <result property="fieldId" column="field_id" jdbcType="INTEGER"/>
        <result property="layoutId" column="layout_id" jdbcType="INTEGER"/>
        <result property="ver" column="ver" jdbcType="INTEGER"/>
        <result property="descr" column="descr" jdbcType="VARCHAR"/>
        <result property="definitionJson" column="definition_json" jdbcType="VARCHAR"/>
        <result property="deleted" column="deleted" jdbcType="BOOLEAN"/>
        <result property="dt" column="dt" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="tableName">
        `id`
    </sql>

    <sql id="baseColumn">
        `id`, `category`, `form_name`, `form_code`, `table_name`, `field_id`, `layout_id`, `ver`, `descr`, `definition_json`, `deleted`, `dt`
    </sql>

</mapper>
